          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT Stud_Course.Grade, Course.Crs_Name, Student.St_Fname,Student.St_Lname
FROM     Course INNER JOIN
                  Stud_Course ON Course.Crs_Id = Stud_Course.Crs_Id INNER JOIN
                  Student ON Stud_Course.St_Id = Student.St_Id
where Student.St_Id=@sid</CommandText>
      </Query>
